{
    "expo": {
        "name": "Pediatric PTSD",
        "slug": "pch-ptsd",
        "privacy": "public",
        "sdkVersion": "35.0.0",
        "platforms": ["ios", "android", "web"],
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/icon.png",
        "ios": {
            "icon": "./assets/icon.png",
            "supportsTablet": true
        },
        "android": {
            "package": "org.aftertheinjury.pchptsd",
            "adaptiveIcon": {
                "foregroundImage": "./assets/android/icon.png",
                "backgroundColor": "#E4002B"
            }
        },
        "splash": {
            "image": "./assets/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "assetBundlePatterns": ["**/*"]
    }
}
